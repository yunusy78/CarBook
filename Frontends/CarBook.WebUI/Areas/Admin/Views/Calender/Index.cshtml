@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "_AdminLayout";
}

<div class="page-content">
    <div class="container-fluid">

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box d-flex align-items-center justify-content-between">
                    <h4 class="mb-0 font-size-18">Kalender</h4>

                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">Sider</a></li>
                            <li class="breadcrumb-item active">Kalender</li>
                        </ol>
                    </div>
                    
                </div>
            </div>
        </div>     
        <!-- end page title -->

        <div class="row">
            <div class="col-12">
                <div class="card m-b-30">
                    <div class="card-body">

                        <div class="row">
                            <div class="col-lg-3 col-md-4">

                                <h6 class="mb-3">Opprett Hendelser</h6>
                                <form method="post" id="add_event_form">
                                    <input style="color: #b02a37; background: #0b0b0b" type="text" class="form-control new-event-form" placeholder="Opprett ny hendelse..." />
                                </form>

                                <div id='external-events' class="mt-4">
                                    <h6 class="mb-3">Dra-og-slipp Hendelser</h6>
                                </div>

                                <!-- sjekkboks -->
                                <div class="custom-control custom-checkbox mt-3">
                                    <input type="checkbox" class="custom-control-input" id="drop-remove" data-parsley-multiple="groups" data-parsley-mincheck="2">
                                    <label class="custom-control-label" for="drop-remove">Fjern etter dra og slipp</label>
                                </div>

                            </div>

                            <div id='calendar' class="col-lg-9 col-md-8 mt-3 mt-lg-0"></div>

                        </div>
                        <!-- end row -->

                    </div>
                </div>
            </div> <!-- end col -->
        </div> <!-- end row --> 

    </div> <!-- container-fluid -->
</div>
<!-- End Page-content -->
```
